{
  "name": "cashier",
  "version": "2.0.7",
  "private": true,
  "main": "main.js",
  "scripts": {
    "start": "npm run start-app",
    "start-app": "SET NODE_ENV=development && nodemon ./bin/www",
    "ele": "electron .",
    "electron:build": "electron-builder",
    "build": "electron-builder"
  },
  "dependencies": {
    "@escook/express-joi": "^1.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "debug": "~2.6.9",
    "electron-updater": "^6.1.7",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron": "^28.2.1",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.gendy.cashier",
    "productName": "cashier",
    "publish": {
      "provider": "github",
      "releaseType": "release",
      "owner": "Aubreyhet",
      "repo": "serverv2"
    },
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    },
    "win": {
      "target": [{
        "target": "nsis"
      }],
      "artifactName": "${productName}${version}.${ext}"
    }
  }
}
